{
  "expo": {
    "name": "PandasJp",
    "slug": "PandasJp",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pandasapps",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "pandasappsgobal",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pandasapps.PandasJp",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pandasapps.PandasJp"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a4b2011b-6bda-4f4a-80de-23afdcf34342"
      }
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/a4b2011b-6bda-4f4a-80de-23afdcf34342"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}