{
  "expo": {
    "name": "PandasJp",
    "slug": "PandasJp",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pandasapps",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "pandasappsgobal",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pandasapps.PandasJp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "GADApplicationIdentifier": "ca-app-pub-8778852534152395~8255277423",
        "NSUserTrackingUsageDescription": "This app uses tracking data to provide personalized ads.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pandasapps.PandasJp",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8778852534152395~8255277423"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a4b2011b-6bda-4f4a-80de-23afdcf34342"
      }
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/a4b2011b-6bda-4f4a-80de-23afdcf34342"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}